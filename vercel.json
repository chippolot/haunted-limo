{
  "routes": [
    {
      "src": "/",
      "dest": "api/index.go"
    },
    {
      "src": "/api/cron",
      "dest": "api/cron.go"
    },
    {
      "src": "/blunders",
      "dest": "api/story.go?storyType=misunderstanding"
    },
    {
      "src": "/whammies",
      "dest": "api/story.go?storyType=slapstick"
    },
    {
      "src": "/hexes",
      "dest": "api/story.go?storyType=curse"
    },
    {
      "src": "/miscreants",
      "dest": "api/story.go?storyType=creature"
    },
    {
      "src": "/nothings",
      "dest": "api/story.go?storyType=antihumor"
    }
  ],
  "functions": {
    "api/*.go": {
      "includeFiles": "data/**/*.*"
    }
  }
}