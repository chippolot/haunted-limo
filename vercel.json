{
  "routes": [
    {
      "src": "/",
      "dest": "api/index.go"
    },
    {
      "src": "/blunders",
      "dest": "api/blunders.go"
    }
  ],
  "functions": {
    "api/*.go": {
      "includeFiles": "data/templates/*.html"
    }
  },
  "crons": [
    {
      "path": "/api/blunders_cron.go",
      "schedule": "0 0 * * *"
    }
  ]
}